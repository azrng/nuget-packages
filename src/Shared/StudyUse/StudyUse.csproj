<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net8.0;net9.0;net10.0</TargetFrameworks>
        <Authors>Azrng</Authors>
        <Copyright>版权归Azrng所有</Copyright>
        <Description>学习使用的nuget包</Description>
        <Version>1.2.0</Version>
        <PackageId>StudyUse</PackageId>
        <GenerateDocumentationFile>True</GenerateDocumentationFile>
        <DocumentationFile>StudyUse.xml</DocumentationFile>
        <LangVersion>latest</LangVersion>
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
        <RepositoryUrl>https://github.com/azrng/nuget-packages</RepositoryUrl>
        <NoWarn>1701;1702;1591</NoWarn>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <IsAotCompatible Condition="'$(TargetFramework)' == 'net8.0' OR '$(TargetFramework)' == 'net9.0' OR '$(TargetFramework)' == 'net10.0'">true</IsAotCompatible>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="AzrngCommon.Security" Version="1.2.0" />
    </ItemGroup>

    <ItemGroup>
        <None Include="README.md" Pack="true" PackagePath="\"/>
    </ItemGroup>
</Project>
